{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://regioni.local/schema/FeatureCollection",
  "title": "GeoJSON FeatureCollection",
  "type": "object",
  "required": ["type", "features"],
  "additionalProperties": false,
  "properties": {
    "type": {
      "type": "string",
      "enum": ["FeatureCollection"]
    },
    "features": {
      "type": "array",
      "items": {

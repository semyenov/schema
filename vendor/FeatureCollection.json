                "type": "object",
                "required": ["type", "geometries"],
                "additionalProperties": false,
                "properties": {
                  "type": {
                    "type": "string",
                    "enum": ["GeometryCollection"]
                  },
                  "geometries": {
                    "type": "array",
                    "items": {
                      "oneOf": [
                        {
                          "title": "GeoJSON Point",
                          "type": "object",
                          "required": ["type", "coordinates"],
                          "additionalProperties": false,
                          "properties": {
                            "type": {
                              "type": "string",
                              "enum": ["Point"]
